{
    "version": 2,
    "builds": [
      { "src": "server.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/server.js" }
    ],
    "env": {
      "HOST": "@HOST",
      "USER": "@USER",
      "PASSWORD": "@PASSWORD",
      "DATABASE": "@DATABASE",
      "URI": "@URI"
    }
  }
  
